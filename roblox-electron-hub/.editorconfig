; http://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 160
ij_javascript_space_before_method_parentheses = true

[*.md]
indent_size = 4

[*.template]
insert_final_newline = false
